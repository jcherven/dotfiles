{
  "codeLens.enable": false,
  "tsserver.implicitProjectConfig.experimentalDecorators": true,
  "highlight.disableLanguages": ["xml", "html", "html.handlebars"],
  "emmet.includeLanguages": { "javascript": "javascriptreact" },
  "markdownlint.config": {
    "line_length": false
  },
  "javascript.referencesCodeLens.enable": false,
  "python.jediEnabled": false,
  "explorer.quitOnOpen": true,
  "explorer.width": 30,
  "explorer.keyMappings.global": {
    "v": "open:vsplit"
  },
  "sql.lintOnOpen": true,
  "sql.lintOnChange": true,
  "sql.lintOnSave": true
}
